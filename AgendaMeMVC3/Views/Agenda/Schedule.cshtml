@model IEnumerable<Telerik.Web.Mvc.UI.Calendar>

@{
    ViewBag.Title = "Schedule";
}

<h2>Schedule</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            Value
        </th>
        <th>
            MinDate
        </th>
        <th>
            MaxDate
        </th>
        <th>
            Name
        </th>
        <th>
            AssetKey
        </th>
        <th>
            ScriptFilesPath
        </th>
        <th>
            IsSelfInitialized
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Value)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MinDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MaxDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AssetKey)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ScriptFilesPath)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsSelfInitialized)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
